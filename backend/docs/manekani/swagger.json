        "/api/v1/subject/{name}": {
            "get": {
                "description": "Search a subject by its name",
                "consumes": [
                    "*/*"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "cards",
                    "subject"
                ],
                "summary": "Query a subject",
                "operationId": "get-subject-query",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Subject name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/cards.Subject"
                        }
                    }
                }
            },
            "delete": {
                "description": "Delete a subject by its name",
