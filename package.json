  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env NODE_ENV=development nest start  --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.ts --detectOpenHandles",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@deepkit/type": "1.0.1-alpha.97",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
